					<tbody>
						{{for geneIdList}}
							<tr>
								<td>Gene Id: {{>geneId}}</td>
								<td><a target="_blank" href="https://www.ncbi.nlm.nih.gov/gene/{{>geneId}}"><button type="button" class="btn btn-info">Ncbi Info</button></a></td>
							</tr>
						{{/for}}
					</tbody>
				</table>
			</div>
		{{if #index%3==2}}
		</div>
